#:kivy 1.9.2

<ElementsWindow>:
    id: mainbox
    orientation: 'vertical'
    BoxLayout:
        size_hint_min_y: 0.05
        size_hint_max_y: 0.1
        #height: '30dp'
        orientation: 'horizontal'
        Button:
            size_hint_x: 0.1
            text: "Play"
        Button:
            size_hint_x: 0.1
            text: "Stop"
    BoxLayout:
        orientation: 'horizontal'
